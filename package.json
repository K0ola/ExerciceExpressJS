{
    "name": "04_prez_expressjs",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon -e ts,html,css,js,png  --exec \"npm run build && npm run start\" --watch src",
        "postbuild": "copyfiles -u 1 src/**/*.{html,css,js,png,jpg} dist/"
    },
    "dependencies": {
        "express": "4.21.1",
        "uuid": "10.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.0.3",
        "@types/express": "^5.0.0",
        "@types/uuid": "10.0.0",
        "copyfiles": "2.4.1",
        "nodemon": "^3.1.0",
        "typescript": "5.6.3"
    }
}
